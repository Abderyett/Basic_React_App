        pages,\n        setPages,\n        loading,\n        setLoading,\n        fetchMovie,\n        showModal,\n        setShowModal,\n      }}\n    >\n      {children}\n    </AppContext.Provider>\n  );\n}\n\nexport { AppContext, AppProvider };\n\nAppProvider.propTypes = PropTypes.element.isRequired;\n\nexport const useGlobalContext = () => useContext(AppContext);\n","/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Loading.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/index.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/CircleSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/SearchSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/logo.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/Home.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Error.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/SingleMovie.js",["124","125"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Modal.js",["126"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/Times.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Cast.js",["127"],"import React, { useState, useEffect } from 'react';\nimport styled from 'styled-components';\nimport PropTypes from 'prop-types';\nimport axios from 'axios';\nimport { slice } from 'lodash';\nimport { motion } from 'framer-motion';\nimport { color, shadow, rounded } from '../utilities';\nimport { Loading } from './Loading';\nimport { useGlobalContext } from '../context';\nimport noImage from '../images/noActorImage1.png';\n\nexport function Cast({ movieID }) {\n  const { loading, setLoading } = useGlobalContext();\n  const [actors, setActors] = useState([]);\n  const [limit, setLimit] = useState(6);\n  const url = `https://api.themoviedb.org/3/movie/${movieID}/credits?api_key=${process.env.REACT_APP_API_KEY}&language=en-US`;\n  const fetchActors = async () => {\n    setLoading(true);\n    try {\n      const { data } = await axios.get(url);\n      setActors(data.cast);\n    } catch (error) {\n      console.log(error);\n    }\n    setLoading(false);\n  };\n\n  useEffect(() => {\n    fetchActors();\n  }, [movieID]);\n\n  return (\n    <div>\n      {loading && <Loading />}\n      <StyledTable initial={{ opacity: 0 }} animate={{ opacity: 1 }} transition={{ duration: 1.5 }}>\n        <TableHead>\n          <tr>\n            <th colSpan=\"3\">Cast</th>\n          </tr>\n        </TableHead>\n\n        {slice(actors, 0, limit).map((actor) => {\n          const { id, character, name, profile_path: image } = actor;\n          return (\n            <TableBody key={id} initial={{ opacity: 0 }} animate={{ opacity: 1 }} transition={{ duration: 1.5 }}>\n              <tr>\n                <td>\n                  <img src={image ? `https://image.tmdb.org/t/p/w92${image}` : noImage} alt={name} />\n                </td>\n                <td>{name}</td>\n                <td>{character}</td>\n              </tr>\n            </TableBody>\n          );\n        })}\n      </StyledTable>\n      {actors.length !== limit && (\n        <StyledButton type=\"button\" onClick={() => setLimit((prev) => prev + (actors.length - prev))}>\n          Load More...\n        </StyledButton>\n      )}\n    </div>\n  );\n}\n\nconst StyledTable = styled(motion.table)`\n  border-bottom: 0.5px solid ${color.grey_400};\n  border-radius: ${rounded.lg};\n  border-collapse: collapse;\n  color: ${color.blue_grey_700};\n  font-size: 1.5rem;\n  width: 40vw;\n  max-width: 580px;\n  margin: 2rem;\n  font-family: 'Sans Regular';\n`;\n\nconst TableHead = styled.thead`\n  tr,\n  th {\n    border-bottom: 2px solid ${color.grey_500};\n    box-shadow: ${shadow.md};\n    text-align: left;\n    padding: 1rem;\n    font-size: 2rem;\n    font-family: 'Sans Regular';\n  }\n`;\nconst TableBody = styled(motion.tbody)`\n  /* &:nth-child(2n + 1) {\n    background: ${color.grey_200};\n  } */\n  tr,\n  td {\n    border-bottom: 1px solid ${color.grey_500};\n\n    text-align: left;\n    padding: 0.4rem;\n    font-family: 'Sans Regular';\n    img {\n      height: 45px;\n      object-fit: contain;\n      border-radius: ${rounded.sm};\n    }\n  }\n`;\n\nconst StyledButton = styled.button`\n  margin-left: 2rem;\n  margin-bottom: 2rem;\n  background-color: transparent;\n  color: ${color.blue_500};\n  font-size: 1.5rem;\n  font-weight: 700;\n  &:hover {\n    color: ${color.blue_400};\n    cursor: pointer;\n  }\n`;\n\nCast.propTypes = PropTypes.number.isRequired;\n","/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/NavHome.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/Link.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/MovieDetails.js",[],{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","replacedBy":"135"},{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","severity":1,"message":"141","line":37,"column":6,"nodeType":"142","endLine":37,"endColumn":13,"suggestions":"143"},{"ruleId":"144","severity":1,"message":"145","line":19,"column":19,"nodeType":"146","messageId":"147","endLine":19,"endColumn":29},{"ruleId":"140","severity":1,"message":"148","line":38,"column":6,"nodeType":"142","endLine":38,"endColumn":8,"suggestions":"149"},{"ruleId":"14