al }}\n    >\n      {children}\n    </AppContext.Provider>\n  );\n}\n\nexport { AppContext, AppProvider };\n\nAppProvider.propTypes = PropTypes.element.isRequired;\n\nexport const useGlobalContext = () => useContext(AppContext);\n","/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Loading.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/index.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/CircleSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/SearchSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/logo.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/Home.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Error.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/SingleMovie.js",["109","110"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Modal.js",["111","112","113"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/Times.js",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":8,"column":11,"nodeType":"128","messageId":"129","endLine":8,"endColumn":15},{"ruleId":"130","severity":1,"message":"131","line":37,"column":6,"nodeType":"132","endLine":37,"endColumn":13,"suggestions":"133"},{"ruleId":"130","severity":1,"message":"134","line":35,"column":6,"nodeType":"132","endLine":35,"end