es/Colors.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/utilities/Radius.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Hero.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/index.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/CardList.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Card.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/context.js",["124"],"import React, { useState, useEffect, useContext } from 'react';\nimport PropTypes from 'prop-types';\nimport axios from 'axios';\n\nconst AppContext = React.createContext();\n\nfunction AppProvider({ children }) {\n  const [term, setTerm] = useState('');\n  const [pages, setPages] = useState(1);\n  const [movies, setMovies] = useState([]);\n  const [loading, setLoading] = useState(false);\n  const [showModal, setShowModal] = useState(false);\n\n  const movieUrl = `https://api.themoviedb.org/3/discover/movie?api_key=${process.env.REACT_APP_API_KEY}&language=en-US&sort_by=popularity.desc&include_adult=false&include_video=false&page=${pages}`;\n  const searchUrl = `https://api.themoviedb.org/3/search/movie?api_key=${process.env.REACT_APP_API_KEY}&query=${term}&page=${pages}`;\n\n  // Fetch Movies\n\n  const fetchMovie = async () => {\n    setLoading(true);\n    try {\n      const { data } = await axios.get(term === '' ? movieUrl : searchUrl);\n\n      if (pages === 1) {\n        setMovies(data.results);\n      } else {\n        setMovies((oldMovies) => [...oldMovies, ...data.results]);\n      }\n      setLoading(false);\n    } catch (error) {\n      console.log('Oh No There is An Error', error);\n    }\n  };\n\n  useEffect(() => {\n    fetchMovie();\n  }, [pages]);\n\n  return (\n    <AppContext.Provider\n      value={{\n        movies,\n        setMovies,\n        term,\n        setTerm,\n        pages,\n        setPages,\n        loading,\n        setLoading,\n        fetchMovie,\n        showModal,\n        setShowModal,\n      }}\n    >\n      {children}\n    </AppContext.Provider>\n  );\n}\n\nexport { AppContext, AppProvider };\n\nAppProvider.propTypes = PropTypes.element.isRequired;\n\nexport const useGlobalContext = () => useContext(AppContext);\n","/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Loading.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/index.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/CircleSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/SearchSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/logo.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/Home.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Error.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/SingleMovie.js",["125","126"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Modal.js",["127"],"import React, { useEffect } from 'react';\nimport ReactDOM from 'react-dom';\nimport styled from 'styled-components';\nimport PropTypes from 'prop-types';\nimport { useGlobalContext } from '../context';\nimport { Times } from './svg';\n\nexport function Modal({ videoId }) {\n  const { setShowModal } = useGlobalContext();\n\n  const closeModal = () => {\n    setShowModal(false);\n  };\n  function closeWithEsc(e) {\n    if (e.key === 'Escape') {\n      setShowModal(false);\n    }\n  }\n\n  useEffect(() => {\n    document.addEventListener('mousedown', closeModal);\n    document.addEventListener('keyup', closeWithEsc);\n\n    return () => {\n      document.removeEventListener('mousedown', closeModal);\n      document.removeEventListener('keyup', closeWithEsc);\n    };\n  }, []);\n\n  console.log(videoId);\n\n  // {`https://www.youtube.com/embed/${videoId}`}\n  return ReactDOM.createPortal(\n    <StyledModal>\n      <ModalContainer>\n        <TimesContainer>\n          <Times />\n        </TimesContainer>\n        <iframe\n          width=\"560\"\n          height=\"315\"\n          src=\"https://www.youtube.com/embed/5qap5aO4i9A\"\n          title=\"YouTube video player\"\n          frameBorder=\"0\"\n          allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\"\n          allowFullScreen\n        />\n      </ModalContainer>\n    </StyledModal>,\n    document.querySelector('#modal')\n  );\n}\nconst StyledModal = styled.div`\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100vw;\n  height: 100vh;\n  background-color: rgba(0, 0, 0, 0.9);\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  iframe {\n    width: 100%;\n    height: 90%;\n    @media (max-width: 768px) {\n      width: 100%;\n      height: 50%;\n    }\n  }\n`;\n\nconst ModalContainer = styled.div`\n  width: 90%;\n  max-width: 1400px;\n  height: 700px;\n`;\n\nconst TimesContainer = styled.div`\n  display: flex;\n  justify-content: flex-end;\n  margin-right: 9rem;\n  cursor: pointer;\n`;\n\nModal.propTypes = PropTypes.string.isRequired;\n","/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/Times.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Cast.js",["128"],"import React, { useState, useEffect } from 'react';\nimport styled from 'styled-components';\nimport PropTypes from 'prop-types';\nimport axios from 'axios';\nimport { slice } from 'lodash';\nimport { motion } from 'framer-motion';\nimport { color, shadow, rounded } from '../utilities';\nimport { Loading } from './Loading';\nimport { useGlobalContext } from '../context';\nimport noImage from '../images/noActorImage1.png';\n\nexport function Cast({ movieID }) {\n  const { loading, setLoading } = useGlobalContext();\n  const [actors, setActors] = useState([]);\n  const [limit, setLimit] = useState(6);\n  const url = `https://api.themoviedb.org/3/movie/${movieID}/credits?api_key=${process.env.REACT_APP_API_KEY}&language=en-US`;\n  const fetchActors = async () => {\n    setLoading(true);\n    try {\n      const { data } = await axios.get(url);\n      setActors(data.cast);\n    } catch (error) {\n      console.log(error);\n    }\n    setLoading(false);\n  };\n\n  useEffect(() => {\n    fetchActors();\n  }, [movieID]);\n\n  return (\n    <div>\n      {loading && <Loading />}\n      <StyledTable initial={{ opacity: 0 }} animate={{ opacity: 1 }} transition={{ duration: 1.5 }}>\n        <TableHead>\n          <tr>\n            <th colSpan=\"3\">Cast</th>\n          </tr>\n        </TableHead>\n\n        {slice(actors, 0, limit).map((actor) => {\n          const { id, character, name, profile_path: image } = actor;\n          return (\n            <TableBody key={id} initial={{ opacity: 0 }} animate={{ opacity: 1 }} transition={{ duration: 1.5 }}>\n              <tr>\n                <td>\n                  <img src={image ? `https://image.tmdb.org/t/p/w92${image}` : noImage} alt={name} />\n                </td>\n                <td>{name}</td>\n                <td>{character}</td>\n              </tr>\n            </TableBody>\n          );\n        })}\n      </StyledTable>\n      {actors.length !== limit && (\n        <StyledButton type=\"button\" onClick={() => setLimit((prev) => prev + (actors.length - prev))}>\n          Load More...\n        </StyledButton>\n      )}\n    </div>\n  );\n}\n\nconst StyledTable = styled(motion.table)`\n  border-bottom: 0.5px solid ${color.grey_400};\n\n  border-collapse: collapse;\n  color: ${color.blue_grey_700};\n  font-size: 1.5rem;\n  width: 40vw;\n  max-width: 580px;\n  margin: 2rem;\n  font-family: 'Sans Regular';\n  @media (max-width: 768px) {\n    width: 70vw;\n    font-size: 1.25rem;\n  }\n`;\n\nconst TableHead = styled.thead`\n  tr,\n  th {\n    border-bottom: 2px solid ${color.grey_500};\n    box-shadow: ${shadow.shadow};\n    text-align: left;\n    padding: 1rem;\n    font-size: 2rem;\n    font-family: 'Sans Regular';\n    @media (max-width: 768px) {\n      font-size: 1.5rem;\n    }\n  }\n`;\nconst TableBody = styled(motion.tbody)`\n  /* &:nth-child(2n + 1) {\n    background: ${color.grey_200};\n  } */\n  tr,\n  td {\n    border-bottom: 1px solid ${color.grey_500};\n\n    text-align: left;\n    padding: 0.4rem;\n    font-family: 'Sans Regular';\n    img {\n      height: 45px;\n      object-fit: contain;\n      border-radius: ${rounded.sm};\n    }\n  }\n`;\n\nconst StyledButton = styled.button`\n  margin-left: 2rem;\n  margin-bottom: 2rem;\n  background-color: transparent;\n  color: ${color.blue_500};\n  font-size: 1.5rem;\n  font-weight: 700;\n  &:hover {\n    color: ${color.blue_400};\n    cursor: pointer;\n  }\n`;\n\nCast.propTypes = PropTypes.number.isRequired;\n","/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/NavHome.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/Link.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/MovieDetails.js",[],{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"141","severity":1,"message":"142","line":37,"column":6,"nodeType":"1