/src/tutorial/4-forms/setup/2-multiple-inputs.js",[],["55","56","57","58","59"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/5-useRef/setup/1-useRef-basics.js",[],["60","61","62","63","64"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/6-useReducer/setup/index.js",["65"],"import React, { useState, useReducer } from 'react';\nimport { v4 as uuidv4 } from 'uuid';\nimport Modal from './Modal';\nimport { data } from '../../../data';\n// reducer function\nconst reducer = (state, action) => {\n  switch (action.type) {\n    case 'ADD':\n      return {\n        ...state,\n        people: [...state.people, action.payload],\n        showModal: true,\n        modelContent: 'item add',\n      };\n    case 'NO_VALUE':\n      return { ...state, showModal: true, modelContent: 'please enter value' };\n    case 'REMOVE':\n      return {\n        ...state,\n        people: state.people.filter((el) => el.id !== action.payload),\n        showModal: true,\n        modelContent: 'removed item',\n      };\n    case 'CLOSE_MODAL':\n      return {\n        ...state,\n        showModal: false,\n      };\n\n    default:\n      return state;\n  }\n};\n\nconst initialState = {\n  people: [],\n  showModal: false,\n  modelContent: '',\n};\n\nconst Index = () => {\n  const [text, setText] = useState('');\n  const [state, dispatch] = useReducer(reducer, initialState);\n  const closeModal = () => {\n    dispatch({ type: 'CLOSE_MODAL' });\n  };\n  const submitHandler = (e) => {\n    e.preventDefault();\n    if (!text) {\n      dispatch({ type: 'NO_VALUE' });\n    } else {\n      const newItem = { id: uuidv4(), title: text };\n      dispatch({ type: 'ADD', payload: newItem });\n      setText('');\n    }\n  };\n\n  return (\n    <>\n      {state.showModal && <Modal closeModal={closeModal} modelContent={state.modelContent} />}\n      <form className=\"form\" onSubmit={submitHandler}>\n        <div>\n          <input type=\"text\" value={text} onChange={(e) => setText(e.target.value)} />\n        </div>\n\n        <button type=\"submit\">add</button>\n      </form>\n      <article>\n        {state.people.map((el) => (\n          <div className=\"item\" key={el.id}>\n            <h4>{el.title}</h4>\n            <button type=\"submit\" onClick={() => dispatch({ type: 'REMOVE', payload: el.id })}>\n              remove\n            </button>\n          </div>\n        ))}\n      </article>\n    </>\n  );\n};\n\nexport default Index;\n","/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/data.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/6-useReducer/setup/Modal.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/7-prop-drilling/setup/1-prop-drilling.js",["66"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/8-useContext/setup/1-context-api.js",[],{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"67","replacedBy":"77"},{"ruleId":"69","replacedBy":"78"},{"ruleId":"71","replacedBy":"79"},{"ruleId":"73","replacedBy":"80"},{"ruleId":"75","replacedBy":"81"},{"ruleId":"67","replacedBy":"82"},{"ruleId":"69","replacedBy":"83"},{"ruleId":"71","replacedBy":"84"},{"ruleId":"73","replacedBy":"85"},{"ruleId":"75","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":4,"column":10,"nodeType":"89","messageId":"90","endLine":4,"endColumn":14},{"ruleId":"87","severity":1,"message":"91","line":1,"column":17,"nodeType":"89","messageId":"90","endLine":1,"endColumn":25},"lines-around-directive",["92"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],["92"],[],[],[],[],["92"],[],[],[],[],"no-unused-vars","'data' is defined but never used.","Identifier","unusedVar","'useState' is defined but nev