ts/react-advanced-2020/src/tutorial/6-useReducer/setup/index.js",["115"],"import React, { useState, useReducer } from 'react';\nimport { v4 as uuidv4 } from 'uuid';\nimport Modal from './Modal';\nimport { data } from '../../../data';\n// reducer function\nconst reducer = (state, action) => {\n  switch (action.type) {\n    case 'ADD':\n      return {\n        ...state,\n        people: [...state.people, action.payload],\n        showModal: true,\n        modelContent: 'item add',\n      };\n    case 'NO_VALUE':\n      return { ...state, showModal: true, modelContent: 'please enter value' };\n    case 'REMOVE':\n      return {\n        ...state,\n        people: state.people.filter((el) => el.id !== action.payload),\n        showModal: true,\n        modelContent: 'removed item',\n      };\n    case 'CLOSE_MODAL':\n      return {\n        ...state,\n        showModal: false,\n      };\n\n    default:\n      return state;\n  }\n};\n\nconst initialState = {\n  people: [],\n  showModal: false,\n  modelContent: '',\n};\n\nconst Index = () => {\n  const [text, setText] = useState('');\n  const [state, dispatch] = useReducer(reducer, initialState);\n  const closeModal = () => {\n    dispatch({ type: 'CLOSE_MODAL' });\n  };\n  const submitHandler = (e) => {\n    e.preventDefault();\n    if (!text) {\n      dispatch({ type: 'NO_VALUE' });\n    } else {\n      const newItem = { id: uuidv4(), title: text };\n      dispatch({ type: 'ADD', payload: newItem });\n      setText('');\n    }\n  };\n\n  return (\n    <>\n      {state.showModal && <Modal closeModal={closeModal} modelContent={state.modelContent} />}\n      <form className=\"form\" onSubmit={submitHandler}>\n        <div>\n          <input type=\"text\" value={text} onChange={(e) => setText(e.target.value)} />\n        </div>\n\n        <button type=\"submit\">add</button>\n      </form>\n      <article>\n        {state.people.map((el) => (\n          <div className=\"item\" key={el.id}>\n            <h4>{el.title}</h4>\n            <button type=\"submit\" onClick={() => dispatch({ type: 'REMOVE', payload: el.id })}>\n              remove\n            </button>\n          </div>\n        ))}\n      </article>\n    </>\n  );\n};\n\nexport default Index;\n",["116","117","118","119","120"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/data.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/6-useReducer/setup/Modal.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/7-prop-drilling/setup/1-prop-drilling.js",["121"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/8-useContext/setup/1-context-api.js",[],["122","123","124","125","126"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/9-custom-hooks/setup/1-fetch-example.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/9-custom-hooks/setup/2-useFetch.js",["127"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/10-prop-types/setup/index.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/10-prop-types/setup/Product.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/9-custom-hooks/final/2-useFetch.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/11-react-router/setup/index.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/11-react-router/setup/Home.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/11-react-router/setup/People.js",["128"],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/11-react-router/setup/Person.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/react-advanced-2020/src/tutorial/11-react-router/setup/About.js",[],"/Users/macbooktouch