{
  "extends": [
    "wesbos",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "no-console": "off",
    "react/jsx-props-no-spreading": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  },
  "plugins": [
    "jsx-a11y"
  ]
}