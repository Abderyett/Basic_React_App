      setLoading,\n        fetchMovie,\n        showModal,\n        setShowModal,\n      }}\n    >\n      {children}\n    </AppContext.Provider>\n  );\n}\n\nexport { AppContext, AppProvider };\n\nAppProvider.propTypes = PropTypes.element.isRequired;\n\nexport const useGlobalContext = () => useContext(AppContext);\n","/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Loading.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/index.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/CircleSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/SearchSvg.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/logo.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/Home.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Error.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/SingleMovie.js",["123","124"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Modal.js",["125"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/Times.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/Cast.js",["126"],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/NavHome.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/svg/Link.js",[],"/Users/macbooktouch/Desktop/Basics_react_Projects/Advanced_React_Projects/5-Movie_DB/src/components/MovieDetails.js",[],{"ruleId":"127","replacedBy":"1